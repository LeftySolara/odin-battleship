{
  "env": {
    "browser": true,
    "es2021": true,
    "jest/globals": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["jest"],
  "rules": {
    "object-curly-newline": [
      "error",
      {
        "ImportDeclaration": "never",
        "ExportDeclaration": { "multiline": true, "minProperties": 5 }
      }
    ]
  }
}
